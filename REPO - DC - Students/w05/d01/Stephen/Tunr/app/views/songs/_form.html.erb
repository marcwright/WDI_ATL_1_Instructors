<%= form_for [@artist, @song] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :video_url %>
  <%= f.text_field :video_url %>
  <%= f.hidden_field :artist_id, :value => @artist.id %>
  <%= f.submit "GO GO GO SONG", id: "submit" %>
<% end %>
