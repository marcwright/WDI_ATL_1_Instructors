<h1><%= @artist.name %></h1>
<p><%= @artist.genre%></p>
<img src="<%= @artist.photo_url %>">
<ul>
  <% @songs.each do |song| %>
    <li> <%= song.title %> - <%= song.year %>
        <%= button_to "Delete song", @song, method: :delete %>
    </li>
  <% end %>

<%= button_to "Delete artist", @artist, method: :delete, id: "destroy" %>
<p><%= link_to "Add song", new_artist_song_path(@artist) %></p>
