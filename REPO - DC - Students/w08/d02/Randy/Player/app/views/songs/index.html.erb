  <form>
    <fieldset>
      <label>Artist:
        <input type="text" name="artist" class="text-field">
      </label>
      <label>Song:
        <input type="text" name="song" class="text-field">
      </label>
       <input type="submit" value="Search" class="button">
    </fieldset>
  </form>


  <ul>
  <% @songs.each do |song| %>

    <li data-character-id="<%= song.id %>"
      Title: <%= song.title %>
      By: <%= song.title %>
      Preview: <%= song.preview_url %>
    </li>

  <% end %>

  </ul>


