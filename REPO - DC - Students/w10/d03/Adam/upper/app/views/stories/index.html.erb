<h1>Stories</h1>

<form>
  <fieldset>
    <legend> New Story </legend>
    <label>
      Story Title
      <input type="text" name="title" />
    </label>
    <label>
      Story URL
      <input type="text" name="url" />
    </label>
    <button>Save</button>
  </fieldset>
</form>

<div id="container"></div>

<script type="template" id="story-html">
  <h2><%%= story.get('title') %></h2>
  <span> &times; </span>
  <h3><%%= story.get('url')%></h3>
</script>
