<%= form_for @venue do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :allows_explicit_lyrics %>
  <%= f.check_box :allows_explicit_lyrics %>

  <%= f.submit "Submit" %>
<% end %>
