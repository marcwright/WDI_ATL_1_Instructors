<%= form_for @venue do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :allows_profanity %>
  <%= f.text_field :allows_profanity %>

  <%= f.submit "Submit" %>
<% end %>
