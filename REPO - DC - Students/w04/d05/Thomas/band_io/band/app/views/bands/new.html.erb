<h1>Add a band:</h1>

<%= form_for @band do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :genre %>
  <%= f.text_field :genre %>

  <%= f.label :explicit_lyrics %>
  <%= f.text_field :explicit_lyrics %>

  <%= f.label :video_url %>
  <%= f.text_field :video_url %>

  <%= f.submit "Submit" %>

<% end %>


<p><%= link_to "Back to Venues", venues_path %></p>
