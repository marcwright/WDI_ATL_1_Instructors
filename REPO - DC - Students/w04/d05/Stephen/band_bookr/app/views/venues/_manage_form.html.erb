<%= form_for @venue do |f| %>
  <%= f.label :name %>
  <%= f.text_field(:name, size:"32") %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field(:state, size:"2")  %>

  <%= f.label(:allows_explicit_lyrics, "Explicit OK?") %>
  <%= f.check_box :allows_explicit_lyrics %>

  <%= f.submit "Submit" %>
<% end %>
