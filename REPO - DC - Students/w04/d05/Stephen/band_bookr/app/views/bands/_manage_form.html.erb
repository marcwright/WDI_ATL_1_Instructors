<%= form_for @band do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :genre %>
  <%= f.text_field(:genre, size:"10") %>

  <%= f.label(:explicit_lyrics, "Explicit?") %>
  <%= f.check_box :explicit_lyrics %>

  <%= f.label(:video_embed_url, "Video URL") %>
  <%= f.url_field :video_embed_url %>

  <%= f.submit "Submit" %>
<% end %>
