<h1>Create a clown</h1>
<form action="/rollercoaster" method="post">
  <label>
    Name:
      <input type="text" name="name">
  </label>

  <label>
    Top Speed:
      <input type="integer" name="top_speed">
  </label>

  <label>
    Minimum Height:
      <input type="integer" name="min_height">
  </label>

  <label>
    Image URL:
      <input type="text" name="image_url">
  </label>

<button>Create Roller Coaster</button>
</form>
