<h2>Editing Entry</h2>

<form action="/entries/<%= @entry.id %>" method="post">
  <input name="_method" type="hidden" value="patch" >
  <input name="authenticity_token" type="hidden" value="<%= form_authenticity_token %>" >

  <label>Author:<input type="text" name="author" value="<%= @entry.author %>"></label>
  <label>URL:<input type="text" name="photo_url" value="<%= @entry.photo_url %>"></label>
  <label>Caption:<input type="text" name="caption" value="<%= @entry.caption %>"></label>

  <input type="submit" value="Edit!">
</form>
