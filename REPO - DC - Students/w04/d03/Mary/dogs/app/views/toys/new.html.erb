<h1>Form for creating new toy</h1>

<form action="/dogs/<%= params[:dog_id] %>/toys" method="post">
  <label for="name-attribute">Name:</label>
  <input type="text" name="name" id="name-attribute">

  <label for="photo-url-attribute">Photo URL</label>
  <input type="text" name="photo_url" id="photo-url-atttribute">


<input type="submit" value="Create Toy">
</form>
