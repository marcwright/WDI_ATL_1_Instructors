<% title "Users" %>

<table>
  <tr>
    <th>Email</th>
    <th>Password Digest</th>
  </tr>
  <% @users.each do |users| %>
    <tr>
      <td><%= users.email %></td>
      <td><%= users.password_digest %></td>
    </tr>
  <% end %>
</table>

<% if can? :create, User %>
  <p><%= link_to "New User", new_user_path %></p>
<% end %>
