<form action="/" method="get">
  <label>
    Stock Symbol:
    <input type="text" name="symbol" placeholder="Enter ticker">
    <input type="submit" value="Submit">
  </label>
</form>

<% if @last_price %>
  <h2><%= @stock_symbol %></h2>
  <h2><%= @last_price %></h2>
  <h2>High:<%= @high %></h2>
  <h2>Low:<%= @low %></h2>
  <h2>Shares Outstanding:<%= @shares %></h2>
<% end %>
