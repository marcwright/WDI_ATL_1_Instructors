<h1>Add a Venue</h1>
<div class="form">
<%= form_for Venue.new do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
<br>
<br>
  <%= f.label :city %>
  <%= f.text_field :city %>
<br>
<br>
  <%= f.label :state %>
  <%= f.text_field :state %>
<br>
<br>
  <%= f.label :allows_explicit_lyrics %>
  <%= f.check_box :allows_explicit_lyrics %>
<br>
<br>
  <%= f.submit "Submit" %>
<% end %>
</div>
<p><%= link_to "Cancel", venues_path %></p>
