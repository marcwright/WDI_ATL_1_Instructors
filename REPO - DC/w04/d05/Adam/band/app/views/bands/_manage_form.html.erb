<%= form_for @band do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :genre %>
  <%= f.text_field :genre %>

  <%= f.label :explicit_lyrics %>
  <%= f.text_field :explicit_lyrics %>

  <%= f.label :videa_embed_url %>
  <%= f.text_field :videa_embed_url %>

  <%= f.submit "Submit" %>
<% end %>
