
<%= form_for @band do |f| %>
  <%=f.label :name %>
  <%= f.text_field :name %>

  <%=f.label :genre %>
  <%= f.text_field :genre %>

  <%=f.label :video_embed_url %>
  <%=f.text_field :video_embed_url%>

  <%=f.label :explicit_lyrics %>
  <%=f.check_box :explicit_lyrics, {:checked=>true}%>

  <%= f.submit "Add new band" %>
<% end %>


