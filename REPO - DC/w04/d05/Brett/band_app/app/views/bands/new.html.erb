<h1>Add a Band</h1>

<%= form_for @band do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :genre %>
  <%= f.text_field :genre %>

  <%= f.label :has_explicit_lyrics %>
  <%= f.text_field :explicit_lyrics %>

  <%= f.label :video_embed_url %>
  <%= f.text_field :video_embed_url %>

  <%= f.submit "Submit" %>
<% end %>

<p><%= link_to "Cancel", bands_path %></p>
